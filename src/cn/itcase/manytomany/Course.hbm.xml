<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  	<class name="cn.itcase.manytomany.Course" table="course">
  		<id name="id">
  			<generator class="native"></generator>
  		</id>
  		<property name="cname"></property>
  		<!-- 多对多关系 -->
  		<set name="stu" table="student_course">
  			<!-- 当前对象在第三方关系表中的外键 -->
  			<key column="cid"></key>
  			<!-- 对方在第三方关系表中的外键 -->
  			<many-to-many class="cn.itcase.manytomany.Student" column="sid"></many-to-many>
  		</set>
  	</class>
</hibernate-mapping>